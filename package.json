{
    "name": "conectima-mobile-app",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "yarn lint && react-scripts build",
        "eject": "react-scripts eject",
        "build:ios": "yarn build && yarn add:ios && yarn open:ios",
        "build:android": "yarn build && yarn add:android && yarn open:android",
        "add:ios": "npx cap add ios || npx cap sync ios",
        "run:ios": "npx cap run ios",
        "open:ios": "npx cap open ios",
        "add:android": "npx cap add android || npx cap sync android",
        "run:android": "npx cap run android",
        "open:android": "npx cap open android",
        "lint": "yarn lint:tsx && yarn lint:scss",
        "lint:tsx": "yarn eslint '**/*.{tsx,ts}'",
        "lint:scss": "yarn stylelint '**/*.scss'",
        "e2e": "cypress run --config video=false",
        "cypress": "cypress open"
    },
    "dependencies": {
        "@apollo/client": "^3.3.14",
        "@capacitor/android": "^3.0.0-rc.0",
        "@capacitor/app": "^0.3.6",
        "@capacitor/core": "^3.0.0-rc.0",
        "@capacitor/haptics": "^0.3.6",
        "@capacitor/ios": "^3.0.0-rc.0",
        "@capacitor/keyboard": "^0.5.6",
        "@capacitor/status-bar": "^0.4.6",
        "@capacitor/storage": "^0.3.6",
        "@ionic/react": "^5.5.0",
        "@ionic/react-router": "^5.5.0",
        "@types/node": "^12.19.15",
        "@types/react": "^16.14.3",
        "@types/react-dom": "^16.9.10",
        "@types/react-router": "^5.1.11",
        "@types/react-router-dom": "^5.1.7",
        "formik": "^2.2.6",
        "graphql": "^15.5.0",
        "i18next": "^20.1.0",
        "ionicons": "^5.4.0",
        "jwt-decode": "^3.1.2",
        "moment": "^2.29.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-i18next": "^11.8.12",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.2",
        "typescript": "^4.1.3",
        "web-vitals": "^0.2.4",
        "winston": "^3.3.3",
        "winston-transport-browserconsole": "^1.0.5",
        "workbox-background-sync": "^5.1.4",
        "workbox-broadcast-update": "^5.1.4",
        "workbox-cacheable-response": "^5.1.4",
        "workbox-core": "^5.1.4",
        "workbox-expiration": "^5.1.4",
        "workbox-google-analytics": "^5.1.4",
        "workbox-navigation-preload": "^5.1.4",
        "workbox-precaching": "^5.1.4",
        "workbox-range-requests": "^5.1.4",
        "workbox-routing": "^5.1.4",
        "workbox-strategies": "^5.1.4",
        "workbox-streams": "^5.1.4",
        "yup": "^0.32.9"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@capacitor/cli": "^3.0.0-rc.0",
        "cypress": "^6.8.0",
        "eslint": "^7.23.0",
        "husky": "4.3.8",
        "node-sass": "^5.0.0",
        "stylelint": "^13.12.0",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-config-standard": "^21.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "description": "An Ionic project"
}
